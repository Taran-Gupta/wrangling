   "source": [
    "#9 represents not applicable and should be null/nan, 4 represents 'other' which are mostly just 'no incarceration' anyways, so convert to \"0\"\n",
    "sentenceLength = sentenceLength.mask(sentenceType == 9, np.nan)\n",
    "sentenceLength = sentenceLength.mask(sentenceType == 4, 0)\n",
    "pd.crosstab(sentenceLength.isnull(),sentenceType)"
   ]
